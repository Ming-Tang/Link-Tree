{
  "name": "Link Tree",
  "version": "1.4",
  "description": "Visualizes the course of your browsing activity.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [ "http://*/*" ],
      "js": [ "content.js" ]
    }
  ]
}
